    "run-script-os": "^1.1.6",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.4",
    "webpack-bundle-analyzer": "^4.6.1"
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
